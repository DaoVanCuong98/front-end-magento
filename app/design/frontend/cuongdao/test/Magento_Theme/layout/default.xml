<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    
    <body>
        <referenceContainer name="top.links" remove = "true" ></referenceContainer>
        
        <referenceContainer name="header.panel">
            <block class="Magento\Framework\View\Element\Template" name="top-header" template="Magento_Theme::html/top-header.phtml"/>
            </referenceContainer>
        <referenceContainer name = "header-wrapper">
            <!-- <block class="Magento\Framework\View\Element\Template" name="logo" template="Magento_Theme::html/logo.phtml"/> -->
            <container name="minicart-test" label="minicart-test-1" htmlTag="div" htmlClass="minicart-test-1">
                
            </container>
        
        </referenceContainer>
        <move element="minicart" destination="minicart-test" before="my-account-link"/>
        <move element="register-link" destination="minicart-test" />
        <move element="my-account-link" destination="minicart-test" />
        <referenceContainer name = "footer-container">
                <block class="Magento\Framework\View\Element\Template" name="footer" template="Magento_Theme::html/footer.phtml"/>
        </referenceContainer>
    </body>

    
</page>